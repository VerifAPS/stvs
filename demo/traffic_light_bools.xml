<code>
<plaintext>PROGRAM PedestrianTrafficLight

  VAR_INPUT
    pedBtnPress : BOOL;
  END_VAR
  
  VAR_OUTPUT
    carLight : BOOL;
    pedLight : BOOL;
    counter  : INT;
  END_VAR

  VAR
    buttonPressedBefore : BOOL;
  END_VAR

  IF (NOT buttonPressedBefore) AND pedBtnPress THEN
    carLight := FALSE;
  END_IF

  IF counter = 10 THEN
    pedLight := TRUE;
  ELSIF counter = 40 THEN
    pedLight := FALSE;
  ELSIF counter = 50 THEN
    carLight := TRUE;
  END_IF

  IF (carLight = FALSE) OR (pedLight = FALSE) THEN
    counter := counter + 1;
  ELSE
    counter := 0;
  END_IF
  
  buttonPressedBefore := pedBtnPress;

END_PROGRAM</plaintext>
