<?xml version="1.0"?>

<specification xmlns="http://formal.iti.kit.edu/stvs/logic/io/xml"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://formal.iti.kit.edu/stvs/logic/io/xml ../specification.xsd"
            isConcrete="false" comment="I am a table comment!">
    <variables>
        <ioVariable io="input" name="A" data-errorType="INT" colwidth="10"></ioVariable>
        <ioVariable io="input" name="B" data-errorType="BOOL"
                    comment="I am a column comment!"></ioVariable>
        <ioVariable io="output" name="C" data-errorType="INT"></ioVariable>
        <ioVariable io="output" name="D" data-errorType="enumD" colwidth="50"
                    comment="I am another column comment!"></ioVariable>
        <freeVariable name="p" data-errorType="INT" default="3"></freeVariable>
    </variables>
    <rows>
        <row><!-- Hier ist die Reihenfolge der Cells wichtig! -->
            <duration comment="I am a duration comment!">[1,5]</duration>
            <cell comment="I am a cell comment!">5</cell>
            <cell>FALSE</cell>
            <cell>A+2</cell>
            <cell comment="I am another cell comment!">literalOne</cell>
        </row>
        <row comment="I am a row comment!">
            <duration>[1,1]</duration>
            <cell>C[-1]</cell>
            <cell>!B[-1]</cell>
            <cell>A</cell>
            <cell>literalTwo</cell>
        </row>
    </rows>
    <enumTypes>
        <enum name="enumD">
            <literal>literalOne</literal>
            <literal>literalTwo</literal>
        </enum>
    </enumTypes>
</specification>